{
  "expo": {
    "name": "ClassHub",
    "slug": "classhub",
    "scheme": "classhub", 
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A7EA9D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.github.caiaocesar.app",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.207695360729-tfodeknp9geq8o55e2pb3bmg3pj5097p"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLName": "io.github.caiaocesar.app"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#A7EA9D"
      },
      "package": "io.github.caiaocesar.app",
      "permissions": [
        "INTERNET" 
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "classhub"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.207695360729-tfodeknp9geq8o55e2pb3bmg3pj5097p",
          "android": {
            "packageName": "io.github.caiaocesar.app"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f8d30428-d531-4dae-b2c6-87b4a9a7723e"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "caiaocesar"
  }
}